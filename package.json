{
  "name": "@wizdom/spfx-ui-fabric-vue",
  "version": "1.0.0",
  "description": "Office ui fabric components made specific for SPFx.",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "build:watch": "rollup -c ./rollup.config.js --watch",
    "test": "cypress run",
    "test:open": "cypress open",
    "docs": "webpack-dev-server --config ./docs/webpack.config.js",
    "docs:build": "webpack --config ./docs/webpack.config.js",
    "dev": "concurrently --kill-others \"npm run build:watch\" \"npm run docs\" \"npm run test:open\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wizdom-Intranet/spfx-ui-fabric-vue.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wizdom-Intranet/spfx-ui-fabric-vue/issues"
  },
  "homepage": "https://github.com/Wizdom-Intranet/spfx-ui-fabric-vue#readme",
  "dependencies": {
    "@uifabric/icons": "^6.0.2",
    "office-ui-fabric-core": "5.0.1",
    "office-ui-fabric-js": "git+https://github.com/zonique2k/office-ui-fabric-js.git",
    "office-ui-fabric-vue": "git+https://github.com/zonique2k/office-ui-fabric-vue.git",
    "spfx-uifabric-themes": "^0.2.1"
  },
  "devDependencies": {
    "@microsoft/loader-load-themed-styles": "^1.7.38",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
